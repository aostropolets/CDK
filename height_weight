SELECT DISTINCT PATIENT_ID AS person_id
			,observation_date AS eventStartDate
			,NULL AS eventEndDate
			,observation_concept_id AS eventConceptId
			,'LabHeight' AS eventType
			,case when unit_concept_id = 8533 then value_as_number*2.54 --	Inches
			when unit_concept_id = 8547 then value_as_number*100 -- m
			when unit_concept_id = 8582 then value_as_number--cm
			else null end AS eventNumValue
		       ,value_as_string AS eventStringValue
		FROM MEASUREMENT 
		WHERE measurement_concept_id IN (select concept_id from ckd_codes where category='height')
			 AND value_as_number IS NOT NULL AND ISNUMERIC(value_as_number) =1
			AND PATIENT_ID IN (SELECT DISTINCT PATIENT_ID FROM $cohortListTable)	
		
			 
