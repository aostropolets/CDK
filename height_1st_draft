CREATE TABLE height
as
SELECT DISTINCT person_id,
			measurement_start_date,
			measurement_concept_id,
			,case when unit_concept_id = 8533 then value_as_number*2.54 --	Inches
			when unit_concept_id = 8547 then value_as_number*100 -- m
			when unit_concept_id = 8582 then value_as_number--cm
			else null end AS value_as_number,
		    value_as_string
		FROM MEASUREMENT 
		WHERE measurement_concept_id IN (select concept_id from ckd_codes 
        where category='height')
;
